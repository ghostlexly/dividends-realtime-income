<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Dividends realtime income viewer !{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}

        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />

        {% block stylesheets %}
            <link rel="stylesheet" href="/assets/css/main.css" />
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            <script src="/assets/js/main.js"></script>
            <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
            <script src="/assets/js/income_calc.js"></script>
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>

    <body class="is-preload">
        {% block body %}{% endblock %}

        <!-- Footer -->
        <footer id="footer">
            <ul class="icons">
                <li><a href="https://www.facebook.com/dubseven7777/" class="icon brands fa-twitter"><span class="label">Facebook</span></a></li>
                <li><a href="mailto:ghostlexly@gmail.com" class="icon fa-envelope"><span class="label">Email</span></a></li>
            </ul>
            <ul class="copyright">
                <li>&copy; Dividends Realtime Income.</li><li>Credits: <a href="mailto:ghostlexly@gmail.com">GhostLexly</a></li>
            </ul>
        </footer>
    </body>
</html>
